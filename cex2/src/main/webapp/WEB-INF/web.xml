<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
	<display-name>Archetype Created Web Application</display-name>
	<servlet>
		<servlet-name>fireweb2</servlet-name>
		<servlet-class>org.fireweb.web2.servlet.Fireweb2ActionServlet</servlet-class>
		<init-param>
			<param-name>fireweb2config</param-name>
			<param-value>cex2-config.xml</param-value>
		</init-param>
		<load-on-startup>1</load-on-startup>
	</servlet>
	<servlet-mapping>
		<servlet-name>fireweb2</servlet-name>
		<url-pattern>*.action</url-pattern>
	</servlet-mapping>
</web-app>
