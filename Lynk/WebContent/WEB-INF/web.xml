<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns="http://xmlns.jcp.org/xml/ns/javaee"
	xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd"
	id="WebApp_ID" version="3.1">
	<display-name>Lynk</display-name>
	<servlet>
		<servlet-name>transportServiceServlet</servlet-name>
		<servlet-class>com.lynk.service.servlet.TransportPartnerServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>transportServiceServlet</servlet-name>
		<url-pattern>/transportServices</url-pattern>
	</servlet-mapping>
	<servlet>
		<servlet-name>machineryServiceServlet</servlet-name>
		<servlet-class>com.lynk.service.servlet.MachineryPartnerServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>machineryServiceServlet</servlet-name>
		<url-pattern>/machineryServices</url-pattern>
	</servlet-mapping>
</web-app>